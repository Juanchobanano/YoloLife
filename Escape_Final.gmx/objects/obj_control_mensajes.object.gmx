            <string>switch(estado){
    case 0:
        if(file_exists(working_directory + "Datos.sav")){ // Existe el archivo.
            if(mouse_check_button_pressed(mb_left)){
                if(mouse_x &gt;= room_width/2-120 and mouse_x &lt;= room_width/2-50 and mouse_y &gt;= 230 and mouse_y &lt;= 250+40){ // Si le dio a sí.
                    /* Borrar todos los datos */
                    if(file_exists(working_directory + "Datos.sav")){
                        file_delete(working_directory + "Datos.sav");
                    }
                    /*if(file_exists(working_directory + "Monedas.sav")){
                        file_delete(working_directory + "Monedas.sav");
                    }*/
                    final = 0;
                    Rooms = 0;
                    SavedRoom = game1;
                    Muertes = 0;
                    estado = 1;
                    audio_play_sound(sn_win,1,0);
                    mouse_clear(mb_left);
                }else if(mouse_x &gt;= room_width/2+60 and mouse_x &lt;= room_width/2+90 and mouse_y &gt;= 250 and mouse_y &lt;= 250+20){ // No
                    room_goto(Game_Menu);
                    volver_menu = 1; // Indica que esta regresando al menu.
                    mouse_clear(mb_left);
                    audio_play_sound(sn_muerte,1,0);
                    estado = 0;
                }
            }
        }else{
            if(mouse_check_button_pressed(mb_left)){
                estado = 2;
            }
        }
    break;
    case 4:
        if(!instance_exists(obj_titulo2)){
            background_color = c_black;
            instance_create(room_width/2 - 510/2,room_height/2-108/2,obj_titulo2);
            with obj_copyright instance_destroy();
        }
        contador++;
        if(contador &gt;= 120){
            src_trans(Room_Creditos);
            contador = 0;
        }
    break;
    /* Default */
    default:
        if(mouse_check_button_pressed(mb_left)){
            if(estado!=3 &amp;&amp; estado!=4){
                estado++;
            }
        }
    break;

}

    
    
    
</string>
