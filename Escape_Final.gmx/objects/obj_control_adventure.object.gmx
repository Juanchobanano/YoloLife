            <string>draw_set_alpha(.2);
//draw_roundrect_colour(padding_box,padding_box-10,room_width/2 - padding_box,room_height - padding_box + 20,c_black,c_black,0);
draw_roundrect_colour(
    padding_box, 
    padding_box-10,
    room_width - padding_box,
    room_height - padding_box + 20,
    c_black,
    c_black,
    0
);
draw_set_alpha(1);

draw_set_font(font_adventure);
draw_set_halign(noone);
draw_set_color(c_yellow);
draw_sprite(spr_titulo_ad,nivel - 1,xx-250, 50);


switch(nivel){
    case 1: // Clamud.
        if(!comenzar){
        draw_text(room_width/2,200,"00:35");
        draw_text(room_width/2,240,"00:29");
        draw_text(room_width/2,280,"00:23");
        }
        draw_sprite(spr_clamud,0,120,180);
        
        draw_set_color(c_white);
        draw_set_halign(fa_center);
        draw_set_font(fnt_1);
        if(idioma == "en"){
            draw_text(200,320,"Clamud was the first#player who ever#finished the#story mode!");
        }else if(idioma == "es"){
            draw_text(200,320,"Clamud fue el primer#jugador que completo#el modo historia!");
        }
        draw_set_font(font_adventure);
        draw_set_halign(noone);
    break;
    case 2: // Puro Games.
        draw_sprite(spr_purogames,0,80,150);
        if(!comenzar){
        draw_text(room_width/2,200,"00:36");
        draw_text(room_width/2,240,"00:31");
        draw_text(room_width/2,280,"00:25");
        }
    break;
    case 3: // Camacholis.
        draw_sprite(spr_camacho,0,120,180);
        if(mouse_check_button_pressed(mb_left) and mouse_x &gt;= 120 and mouse_x &lt;= 120 + 174 and mouse_y &gt;= 180 and mouse_y &lt;= 180 + 174){
            url_open('https://www.youtube.com/user/JuanDavidCamacholis');
        }
        if(!comenzar){
        draw_text(room_width/2,200,"00:47");
        draw_text(room_width/2,240,"00:42");
        draw_text(room_width/2,280,"00:35");
        }
        
        draw_set_color(c_white);
        draw_set_halign(fa_center);
        draw_set_font(fnt_1);
        if(idioma == "en"){
            draw_text(220,360,"Visit his channel!#Force Cube :)");
        }else if(idioma == "es"){
            draw_text(220,360,"Visiten su canal!#Force Cube :)");
        }
        draw_set_font(font_adventure);
        draw_set_halign(noone);
    break;
    case 4: // Emo.
        draw_sprite(spr_steven,0,120,180);
        if(!comenzar){
        draw_text(room_width/2,200,"00:25");
        draw_text(room_width/2,240,"00:18");
        draw_text(room_width/2,280,"00:13");
        }
    break;
}


// Dibujar las estrellas que el jugador tiene en ese nivel.
if(!comenzar){
    yy = 320;
}else{
    yy = 240;
}
xx1 = room_width / 2 + 20;


switch(value){
    case 1:
        draw_sprite(spr_star2,0,xx1,yy);
        draw_sprite(spr_star2,1,xx1 + 64 + 30,yy);
        draw_sprite(spr_star2,1,xx1 + 128 + 60,yy);
    break;
    case 2: 
        draw_sprite(spr_star2,0,xx1,yy);
        draw_sprite(spr_star2,0,xx1 + 64 + 30,yy);
        draw_sprite(spr_star2,1,xx1 + 128 + 60,yy);
    break;
    case 3:
        draw_sprite(spr_star2,0,xx1,yy);
        draw_sprite(spr_star2,0,xx1 + 64 + 30,yy);
        draw_sprite(spr_star2,0,xx1 + 128 + 60,yy);
    break;
    default:
        draw_sprite(spr_star2,1,xx1,yy);
        draw_sprite(spr_star2,1,xx1 + 64 + 30,yy);
        draw_sprite(spr_star2,1,xx1 + 128 + 60,yy);
    break;
}

draw_set_halign(noone);
if(!comenzar){
    // Coordenadas.
    xx1 = room_width/2 + string_width("00:00") + 10;
    yy = 180;
    
    // Dibujar estrellas.
    draw_sprite(spr_star,0,xx1,yy);
    draw_sprite(spr_star,1,xx1 + 32 + 5,yy);
    draw_sprite(spr_star,1,xx1 + 64 + 10,yy);
    
    draw_sprite(spr_star,0,xx1,yy + 40);
    draw_sprite(spr_star,0,xx1 + 32 + 5,yy + 40);
    draw_sprite(spr_star,1,xx1 + 64 + 10,yy + 40);
    
    draw_sprite(spr_star,0,xx1,yy + 80);
    draw_sprite(spr_star,0,xx1 + 32 + 5,yy + 80);
    draw_sprite(spr_star,0,xx1 + 64 + 10,yy + 80);
    
}else{
    draw_set_font(c_white);
    draw_set_font(font_adventure);
    draw_text(xx1 + 50, yy - 50, string(mostrar_tiempo) + 's!');

}

// Start, restart.
draw_set_font(font_mensaje);
draw_set_halign(fa_center);
if(idioma == "en"){
    draw_set_color(c_yellow);
    if(!comenzar){
        draw_sprite(spr_start,0,xx - 95, room_height - padding_box - 40);
    }else{
        draw_sprite(spr_restart,0,xx - 115,room_height - padding_box - 40);
    }
}else if(idioma == "es"){
    draw_set_color(c_yellow);
    if(!comenzar){
        draw_sprite(spr_start,1,xx - 160,room_height - padding_box - 40);
    }else{
        draw_sprite(spr_restart,1,xx - 154,room_height - padding_box - 40);
    }
}

// Start - Comenzar
if(idioma == 'en'){
    if(mouse_check_button(mb_left) and ((mouse_x &gt;= xx - 95 and mouse_y &lt;= xx - 95 + 207) 
    and (mouse_y &gt;= room_height - padding_box - 40 and mouse_y &lt;= room_height - padding_box - 40 + 38))){
        // Start Game or Restart it!
        audio_play_sound(sn_win,1,0);
        if(!comenzar){
            instance_destroy();
        }else{
            room_restart();
            mouse_clear(mb_left); 
        }
    }
}else if(idioma == 'es'){
    if(mouse_check_button(mb_left) and ((mouse_x &gt;= xx - 160 and mouse_y &lt;= xx - 160 + 320) 
    and (mouse_y &gt;= room_height - padding_box - 40 and mouse_y &lt;= room_height - padding_box - 40 + 38))){
        // Comenzar Juego!
        audio_play_sound(sn_win,1,0);
        if(!comenzar){
            instance_destroy();
        }else{
            room_restart();
            mouse_clear(mb_left);
        }
    }
}
