        ini_open(working_directory + 'tiempo2.sav');
        Score_tiempo = ini_read_real("tiempo","pass",0);
        ini_close();
    }
        
    /* Lector Noticias */   
    if(file_exists(working_directory + 'noticias2.sav')){
        ini_open(working_directory + 'noticias2.sav');
        noticias = ini_read_real("noticias","pass",0);
        ini_close();
    }
    
    /* Lector de Settings */
    if(file_exists(working_directory + "Settings.sav")){
        ini_open(working_directory + "Settings.sav");
        idioma = ini_read_string("idioma","pass",0);
        ini_close();
    }
    
