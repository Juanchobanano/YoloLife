        Rooms = ini_read_real("Room","pass",0);
        SavedRoom = ini_read_real("SavedRoom","pass",0);
        Muertes = ini_read_real("Muertes","pass",0);
        final = ini_read_real("final","pass",0);
        Best_Muertes = ini_read_real("BestMuertes","pass",0);
        ini_close();
