            <kind>1</kind>
            <string>if(room!=gamex1){
   // monedas += monedas_room;
   /* if(file_exists(working_directory + "Monedas.sav")){
        file_delete(working_directory + "Monedas.sav");
    }
    ini_open(working_directory + "Monedas.sav");
    ini_write_real("Monedas","pass",monedas);
    ini_close();*/
    if(!reto and !aventura){
        Rooms++;
        room_goto_next();
        audio_play_sound(sn_win,1,0);
        // Guardar Monedas.
        monedas += monedas_locales;
        if(file_exists(working_directory + "monedas.sav")){
            file_delete(working_directory + "monedas.sav");
        }
        ini_open(working_directory + "Monedas.sav");
        ini_write_real("Monedas","pass",monedas);
        ini_close();
    }else{
        if(!once){            
            // Sonido de ganar !
            audio_play_sound(sn_win,1,0);
                        
            // Monedas (Actualizar monedas).
            monedas += monedas_locales;
            if(file_exists(working_directory + "monedas.sav")){
                file_delete(working_directory + "monedas.sav");
            }
            ini_open(working_directory + "Monedas.sav");
            ini_write_real("Monedas","pass",monedas);
            ini_close();
            
            // &lt; CHALLENGE MODE &gt;
            if(reto and !aventura){
                // Actualizar variable Score_tiempo.
                if(!file_exists(working_directory + "tiempo2.sav")){
                    Score_tiempo = tiempo; 
                }else{
                    if(tiempo &lt; Score_tiempo){
                        Score_tiempo = tiempo;
                        instance_create(view_xview[0] + (room_width/2)/2 + 60, 300, obj_new);
                        instance_create(view_xview[0] + (room_width/2)/2 - 105, 300, obj_new);
                    }
                }
                // Guardar tiempo.
                if(file_exists(working_directory + 'tiempo2.sav')){
                    file_delete(working_directory + 'tiempo2.sav');
                }
                ini_open(working_directory + 'tiempo2.sav');
                ini_write_real("tiempo","pass",Score_tiempo); 
                ini_close();
                // Crear objeto challenge.
                if(!instance_exists(obj_control_challenge)){
                    ch = instance_create(0,0,obj_control_challenge);
                }
                with(ch){ comenzar = 1; xx = 800 + (room_width/2)/2; } 
                alarm[1] = 1; // Polvora.
                //tiempo = 0;
                
            // &lt; ADVENTURE MODE &gt;
            }else if(!reto and aventura){
            
                ad = instance_create(0,0,obj_control_adventure);
                with (ad) { comenzar = 1 };
                cantidad_estrellas = src_estrellas(nivel,tiempo);
                
                // Cambiar valor en la posición seleccionada.
                if(ds_list_find_value(estrellas,nivel-1) &lt; cantidad_estrellas){
                    ds_list_replace(estrellas,nivel-1,cantidad_estrellas);
                    // Actualizar archivo (estrellas.sav)
                    if(file_exists(working_directory + 'estrellas.sav')){
                        file_delete(working_directory + 'estrellas.sav');
                    }
                    ini_open(working_directory + 'estrellas.sav');
                    ini_write_string("estrellas","pass",ds_list_write(estrellas));
                    ini_close();
                }
                            
                mostrar_tiempo = tiempo;
            }
            // Reiniciar Iniciar.
            iniciar = 0; tiempo = 0;
        once = 1;
        }
    }
}else if(room == gamex1){  // Story Mode.
    final = 1;
    
    // Guardar este valor.
    if(Best_Muertes == 0){
        Best_Muertes = Muertes;
    }else{
        if(Muertes &lt; Best_Muertes){
            Best_Muertes = Muertes;
        }
    }
    GoogleAnalytics_SendEvent("Game Completed",final);
    // Guardar muertes en el marcador.
    if (achievement_login_status() == false &amp;&amp; os_is_network_connected()){
        achievement_login();
    }else{
        //achievement_show_leaderboards();
        if(Best_Muertes != 0){
            achievement_post_score("CgkIn7v1l5McEAIQCA",Best_Muertes);
        }
    }
    background_y[0] = 0;
    src_trans(game_final);
}
